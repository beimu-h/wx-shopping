.index_swiper swiper {
  width: 750rpx;
  height: 340rpx;
}
.index_swiper swiper image {
  width: 100%;
}
.index_cates {
  display: flex;
}
.index_cates navigator {
  padding: 25rpx;
  flex: 1;
}
.index_cates navigator image {
  width: 100%;
}
.index_floor .floor-group {
  padding: 5px 0;
}
.index_floor .floor-group .floor-title image {
  width: 100%;
}
.index_floor .floor-content {
  overflow: hidden;
}
.index_floor .floor-content navigator {
  float: left;
  width: 33.33%;
}
.index_floor .floor-content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 5px solid #fff;
}
.index_floor .floor-content navigator:nth-child(2),
.index_floor .floor-content navigator:nth-child(3) {
  border-bottom: 5px solid #fff;
}
.index_floor .floor-content navigator image {
  width: 100%;
  height: 100%;
}
